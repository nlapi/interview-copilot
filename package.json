{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron:dev": "concurrently \"npm run serve\" \"electron electron.js\"",
    "electron:build": "vue-cli-service build && electron-builder",
    "electron:start": "electron electron.js"
  },
  "main": "electron.js",
  "build": {
    "appId": "com.interviewcopilot.app",
    "productName": "Interview Copilot",
    "files": [
      "dist/**/*",
      "electron.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "public",
      "output": "electron-dist"
    },
    "win": {
      "target": "portable",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "dependencies": {
    "@vue/cli": "^5.0.8",
    "assert-js": "^1.0.0",
    "axios": "^1.8.4",
    "core-js": "^3.41.0",
    "crypto-js": "^4.2.0",
    "element-ui": "^2.15.14",
    "markdown-it": "^13.0.2",
    "marked": "^10.0.0",
    "md5-hex": "^5.0.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.43.1",
    "moment": "^2.30.1",
    "mongodb": "^6.15.0",
    "openai": "^4.93.0",
    "raw-loader": "^4.0.2",
    "recorder-js": "^1.0.7",
    "recordrtc": "^5.6.2",
    "serve": "^14.2.4",
    "universal-cookie": "^6.1.3",
    "vue": "^2.7.16",
    "vue-cli": "^2.9.6",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "vue-template-compiler": "^2.7.16"
  }
}
